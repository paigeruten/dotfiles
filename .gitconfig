[user]
	name = Jeremy Ruten
	email = jeremy.ruten@gmail.com
[color]
	ui = true
[color "diff"]
	whitespace = red reverse
[push]
	default = simple
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/jeremy/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[alias]
	co = checkout
	br = branch
	ci = commit --cleanup=scissors
	cia = commit --amend --cleanup=scissors
	st = status
	logs = log --format='%h %Cblue%s'
	rb = rebase
	rbi = rebase -i
	rbr = rebase -i --root
	rbc = rebase --continue
	rbe = rebase --edit-todo
	bs = bisect
	bss = bisect start
	bsg = bisect good
	bsb = bisect bad
	bsr = bisect reset
